{
	"plugins": [],
	"recurseDepth": 50,
	"source": {
		"include": [],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"includeDate": false
		}
	},
	"opts": {
		"destination": "./docs",
		"recurse": true,
		"readme": "./README.md"
	}
    }